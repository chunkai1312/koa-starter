{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "arrow-body-style": 0,
    "func-names": 0,
    "max-len": 0,
    "new-cap": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "strict": 0
  }
}